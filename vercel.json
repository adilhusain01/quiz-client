{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://your-backend-domain.com/api/:path*"
      }
    ],
    "redirects": [
      {
        "source": "/old-path",
        "destination": "/new-path",
        "permanent": true
      },
      {
        "source": "/another-old-path",
        "destination": "/another-new-path",
        "permanent": false
      }
    ]
  }